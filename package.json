{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "Starter kit for any react app",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --env.development --config config/webpack.config.js",
    "build": "webpack --env.production --progress --config config/webpack.config.js",
    "test": "jest"
  },
  "keywords": [],
  "author": "Gerardo Contreras",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-live-clock": "^3.1.0",
    "react-router-dom": "^5.0.1",
    "reactstrap": "^8.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^24.0.13",
    "autoprefixer": "^9.5.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-mobx": "^2.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "redux-saga-test-plan": "^4.0.0-beta.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "\\.(css|scss|sass)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/testUtils/fileTransformer.js"
    }
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ]
}
